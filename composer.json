{
  "name": "mops1k/json-api",
  "description": "JSON-API responses in PHP",
  "keywords": [
    "json",
    "api",
    "standard",
    "jsonapi"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Toby Zerner",
      "email": "toby.zerner@gmail.com"
    },
    {
      "name": "Aleksandr Kvintilyanov",
      "email": "bednyj.mops@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "JsonApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonApi\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
